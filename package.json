{
  "name": "html-log",
  "version": "1.0.0",
  "description": "Print JavaScript values as HTML",
  "main": "./dist/cjs/html-log.js",
  "module": "./dist/esm/html-log.js",
  "exports": {
    ".": {
      "import": "./dist/esm/html-log.js",
      "require": "./dist/cjs/html-log.js"
    }
  },
  "types": "dist/html-log.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint .",
    "watch": "tsc -w",
