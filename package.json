    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "mocha": "^9.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/html-log.git"
  },
