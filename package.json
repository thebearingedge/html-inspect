    "c8": "^7.11.0",
    "chai": "^4.3.6",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.1",
    "outdent": "^0.8.0",
    "rollup": "^2.69.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
